{
	"name": "type-music",
	"displayName": "Type Music",
	"description": "typing with music",
	"version": "0.0.3",
	"publisher": "xiaomingplus",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/xiaomingplus/type-music"
	},
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.toggle",
				"title": "Toggle Typing Music"
			},
			{
				"command": "extension.choose",
				"title": "Choose Typing Music Playlist"
			},
			{
				"command": "extension.next",
				"title": "Next Music Of Current Typeing Music Playlist"
			},
			{
				"command": "extension.add",
				"title": "Add Music You Liked To Typing Music"
			},
			{
				"command": "extension.open",
				"title": "Open Typing Music Playlist Folers"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.6",
		"tslint": "^5.8.0",
		"@types/node": "^7.0.43",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"find-exec": "^1.0.0",
		"fs-extra": "^7.0.0",
		"node-dir": "^0.1.17"
	},
	"__metadata": {
		"id": "ec5ce87f-16f4-44d3-9020-5a707d9bc97b",
		"publisherId": "f7f33e77-75ce-47f7-8922-3d953424e4cc",
		"publisherDisplayName": "xiaomingplus"
	}
}